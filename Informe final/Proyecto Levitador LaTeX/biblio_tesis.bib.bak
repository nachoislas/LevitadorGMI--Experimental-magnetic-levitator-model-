@Article{Garcia2007,
  Title                    = {Digital-Control-Based Solution to the Effect of Nonidealities of the Inductors in Multiphase Converters},
  Author                   = {Garcia, O. and de Castro, A. and Zumelis, P. and Cobos, J.A.},
  Journal                  = IEEE_J_PWRE,
  Year                     = {2007},

  Month                    = nov,
  Number                   = {6},
  Pages                    = {2155--2163},
  Volume                   = {22},

  Abstract                 = {Multiphase is an old technique that, nowadays, is widely used in several dc-dc applications such as voltage regulator module and automotive. The number of phases is usually kept relatively low (three or four) in most cases. However, the advantages of multiphase converters (mainly the reduction of input and output filters with its associated increase in maximum control bandwidth) grow with the number of phases in the ideal case. This growth is limited by nonidealities, mainly the tolerances of components (semiconductors and inductors). The purpose of this paper is to evaluate the effect of tolerances of the inductance on the current ripple. This nonideality causes the loss of two important advantages of multiphase converters, such as very high frequency and very low amplitude current ripple. The use of digital control can help to reduce drastically this problem. A simple algorithm, based on changing the triggering sequence of the phases, allows a big reduction of the impact of this nonideality. Experiments are included in the paper.},
  Doi                      = {10.1109/TPEL.2007.909406},
  File                     = {:Interleaved\\2007 - Garcia - Digital-Control-Based Solution to the Effect of Nonidealities of the Inductors in Multiphase Converters.pdf:PDF},
  ISSN                     = {0885-8993},
  Keywords                 = {current ripple;digital-control-based solution;inductance;inductors;multiphase converters;digital control;inductors;power convertors;},
  Owner                    = {pabloa},
  Timestamp                = {2014.02.13}
}

@Article{Garcia2009,
  Title                    = {Current Self-Balance Mechanism in Multiphase Buck Converter},
  Author                   = {Garcia, O. and Zumel, P. and de Castro, A. and Alou, P. and Cobos, J.A.},
  Journal                  = IEEE_J_PWRE,
  Year                     = {2009},

  Month                    = jun,
  Number                   = {6},
  Pages                    = {1600--1606},
  Volume                   = {24},

  Abstract                 = {Some of the recent applications in the field of the power supplies use multiphase converters to achieve fast dynamic response, smaller input/output filters, or better packaging. Typically, these converters have several paralleled power stages, with a current loop in each phase and a single voltage loop. The presence of the current loops avoids current imbalance among phases. The purpose of this paper is to demonstrate that, in CCM, with a proper design, there is an intrinsic mechanism of self-balance that reduces the current imbalance. Thus, in the buck converter, if natural zero-voltage switching (ZVS) is achieved in both transitions, the instantaneous inductor current compensates partially the different DC currents through the phases. The need for using n current loops will be finally determined by the application but not by the converter itself. Using the buck converter as a base, a multiphase converter has been developed. Several tests have been carried out in the laboratory and the results show clearly that, when the conditions are met, the phase currents are very well balanced even during transient conditions.},
  Doi                      = {10.1109/TPEL.2009.2013859},
  File                     = {:Interleaved\\2009 - Garcia - Current Self-balance Mechanism in Multiphase Buck  Converter.pdf:PDF},
  ISSN                     = {0885-8993},
  Keywords                 = {dynamic response;inductor current compensation;multiphase buck converter;power supplies;self-balance mechanism;zero-voltage switching;dynamic response;switching convertors;zero voltage switching;},
  Owner                    = {pabloa},
  Timestamp                = {2014.04.16}
}

